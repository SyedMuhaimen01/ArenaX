<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gradient Background -->
    <item>
        <shape>
            <gradient
                android:angle="135"
                android:startColor="#2C3E50"
            android:centerColor="#4A4A4A"
            android:endColor="#000000"
            android:type="linear" />
            <corners android:radius="@dimen/_12sdp" /> <!-- Rounded corners -->
        </shape>
    </item>

    <!-- Row 1: Top-Left Corner -->
    <item>
        <bitmap
            android:src="@drawable/star_outline"
            android:gravity="top|start"
            android:alpha="0.3"
            android:width="40dp"
            android:height="40dp" />
    </item>

    <item>
        <inset
            android:insetTop="5dp"
            android:insetLeft="20dp"
            android:insetRight="0dp"
            android:insetBottom="0dp">
            <bitmap
                android:src="@drawable/star2_filled"
                android:gravity="top|start"
                android:alpha="0.2"
            android:width="20dp"
            android:height="20dp" />
        </inset>
    </item>

